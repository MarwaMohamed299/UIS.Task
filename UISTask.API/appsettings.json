{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "UISTask": "Server=.;Database=TASK ;Trusted_Connection=True; TrustServerCertificate=True; "
  },
  "AllowedHosts": "*"
}